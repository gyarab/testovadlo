#!/bin/bash

function jj { local IFS=":"; echo "$*"; }

main=$(head -1 /data/.mainclass)
jars=( . $(find / -maxdepth 1 -type f -name '*.jar') )

echo "$ java -cp $(jj "${jars[@]#/}") $main"
out=$(cd /data/build; java -cp $(jj "${jars[@]}") "$main")

cat <<<"$out"

a=$(tr ' ' '\n' <<<"$out" | tr ',' '\n' | sort -u|xargs echo)
b="Dd1 Jb1 Jg1 Ke1 pa2 pb2 pc2 pd2 pe2 pf2 pg2 ph2 Sc1 Sf1 Va1 Vh1"

[[ $a == $b ]] || { echo "špatně!"; echo 11; }
